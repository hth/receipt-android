<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter>
    <!-- http://stackoverflow.com/questions/7568579/eclipsefindbugs-exclude-filter-files-doesnt-work -->
    <Match>
        <Class name="~.*\.R\$.*"/>
    </Match>
    <Match>
        <Class name="~.*\.Manifest\$.*"/>
    </Match>
    <!-- All bugs in test classes, except for JUnit-specific bugs -->
    <Match>
        <Class name="~.*\.*Test" />
        <Not>
            <Bug code="RCH" />
        </Not>
    </Match>

    <Match>
        <Package name="com.receiptofi.receipts.model" />
    </Match>

    <Match>
        <Package name="com.receiptofi.receipts.fragments" />
    </Match>

</FindBugsFilter>